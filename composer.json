{
    "name": "magehx/mahx-checkout",
    "description": "Lightweight, fast, and Magento-native checkout solution for Magento 2, built with HTMX, AlpineJS",
    "type": "magento2-module",
    "version": "1.0.0-beta.1",
    "minimum-stability": "dev",
    "license": "MIT",
    "prefer-stable": true,
    "require": {
        "magento/framework": "*",
        "magento/module-catalog": ">=103.0.0",
        "magento/module-checkout": ">=100.3.0",
        "magento/module-customer": ">=102.0.0",
        "rkt/magento-data": "*",
        "magehx/htmx-actions": "*",
        "magehx/mahx-checkout-virtual": "*",
        "magehx/mage-template-utils": "*"
    },
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "MageHx\\MahxCheckout\\": "src/",
            "MageHx\\MahxCheckout\\Tests\\": "tests/"
        }
    }
}
